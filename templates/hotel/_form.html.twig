
{{ form_start(form,{'attr': {'novalidate': 'novalidate'}}) }}

<div class="form-horizontal style-form">

    <div class="form-group">
        <label class="col-sm-2 col-sm-2 control-label">Nom</label>
        <div class="col-sm-10">
            {{ form_widget(form.nom,{'attr': {'class': 'form-control' , 'placeholder' : 'Nom d Hotel'}}) }}
            {{ form_errors(form.nom) }}
        </div>
    </div>

    <div class="form-group">
        <label class="col-sm-2 col-sm-2 control-label">localisation</label>
        <div class="col-sm-10">
            {{ form_widget(form.localisataion,{'attr': {'class': 'form-control' , 'placeholder' : 'localisation'}}) }}
            {{ form_errors(form.localisataion) }}
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 col-sm-2 control-label">discription</label>
        <div class="col-sm-10">
            {{ form_widget(form.description,{'attr': {'class': 'form-control' , 'placeholder' : 'discription'}}) }}
            {{ form_errors(form.description) }}
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 col-sm-2 control-label">Image</label>
        <div class="col-sm-10">
            {{ form_widget(form.image,{'attr': {'class': 'form-control' , 'placeholder' : 'Image'}}) }}
            {{ form_errors(form.image) }}

        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 col-sm-2 control-label">prix</label>
        <div class="col-sm-10">
            {{ form_widget(form.prix,{'attr': {'class': 'form-control' , 'placeholder' : 'prix'}}) }}
            {{ form_errors(form.prix) }}

        </div>
    </div>

    <button class="btn">{{ button_label|default('Save') }}</button>
</div>

{{ form_end(form) }}
